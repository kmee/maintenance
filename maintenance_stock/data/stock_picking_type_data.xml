<odoo>

    <record id="internal_maintenance_picking_type_sequence" model="ir.sequence">
        <field name="name">Sequencia de picking para manutenção</field>
        <field name="implementation">standard</field>
        <field name="prefix">FIX/</field>
    </record>

    <record id="picking_type_internal_maintenance" model="stock.picking.type">
        <field name="name">Internal Maintenance</field>
        <field name="sequence_id" ref="internal_maintenance_picking_type_sequence"/>
        <field name="warehouse_id" ref="stock.warehouse0"/>
        <field name="code">internal</field>
        <field name="return_picking_type_id"></field>
        <field name="barcode_nomenclature_id"></field>
        <field name="use_create_lots"></field>
        <field name="use_existing_lots">True</field>
        <field name="default_location_src_id" ref=""/>
        <field name="default_location_dest_id" ref="stock_location_maintenance"/>
    </record>

</odoo>
