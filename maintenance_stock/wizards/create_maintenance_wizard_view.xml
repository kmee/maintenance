<odoo>

    <record model="ir.ui.view" id="send_maintenance_wizard_view">
        <field name="name">wizard.send.maintenance (in maintenance_stock)</field>
        <field name="model">create.maintenance.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="maintenance_team_id" required="1"/>
                    <field name="description" required="1"/>
                </group>
                <footer>
                    <button name="action_maintenance"
                            string="Confirm"
                            class="btn-primary"
                            type="object"/>
                    <button string="Cancel"
                            class="btn-default"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

